<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
     <application>
        <resource-bundle>
            <base-name>resources/mensajes</base-name>
            <var>msg</var>
        
        </resource-bundle>
    </application>
    <managed-bean>
  <managed-bean-name>DAO</managed-bean-name>
  <managed-bean-class>com.web.myproject</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>visualizacion</managed-bean-name>
  <managed-bean-class>com.web.myproject.Visualizacion_Bean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>etiqueta</managed-bean-name>
  <managed-bean-class>com.web.myproject.EtiquetaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>atributo</managed-bean-name>
  <managed-bean-class>com.web.myproject.AtributoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>registro</managed-bean-name>
  <managed-bean-class>com.web.myproject.Usuarios</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>correo</managed-bean-name>
  <managed-bean-class>com.web.myproject.RegistroBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>completado</managed-bean-name>
  <managed-bean-class>com.web.myproject.AutocompleteBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>pbean</managed-bean-name>
  <managed-bean-class>com.web.myproject.PruebaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
  <managed-bean-name>textarea</managed-bean-name>
  <managed-bean-class>com.web.myproject.TextArea</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<navigation-rule>
	<from-view-id>/faces/Nuevo_RecHTML.xhtml</from-view-id>
	<navigation-case>
		<from-outcome>visualizacionprevia</from-outcome>
		<to-view-id>/faces/visualizacionprevia.html</to-view-id>
	</navigation-case>
</navigation-rule>

</faces-config>
